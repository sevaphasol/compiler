~ func main()
{
    ~ var RUNS    = 1000
    ~ var counter = 5
    ~ var a = 1
    ~ var b = 1

    ~ while (RUNS)
    {
        ~ while (counter)
        {
            ~ var tmp = b
            ~ b = a + b
            ~ a = tmp

            ~ counter = counter - 1
        }

        ~ RUNS = RUNS - 1
    }

    ~ return 0
}
